# ===========================================
# 开发环境配置文件
# AI图像生成器 - 开发环境专用配置
# ===========================================

# 应用基本信息
VITE_APP_TITLE=AI图像生成器(开发版)
VITE_APP_DESCRIPTION=基于AI的智能图像生成平台
VITE_APP_VERSION=1.0.0-dev

# ===========================================
# API服务器配置
# ===========================================

# 开发环境API地址 - 使用本地后端服务
VITE_API_BASE_URL=http://localhost:5004/api
VITE_API_SERVER_URL=http://localhost:5004

# API超时设置（毫秒）
VITE_API_TIMEOUT=60000

# API重试次数
VITE_API_RETRY_COUNT=1

# ===========================================
# 构建优化配置
# ===========================================

# 开发环境生成source map（方便调试）
VITE_BUILD_SOURCEMAP=true

# 开发环境不压缩代码
VITE_BUILD_MINIFY=false

# 启用代码分割
VITE_BUILD_CODE_SPLITTING=true

# ===========================================
# 功能开关配置
# ===========================================

# 开发者模式（开发环境开启）
VITE_DEVELOPER_MODE=true

# 调试模式（开发环境开启）
VITE_DEBUG=true

# 日志级别（error, warn, info, debug）
VITE_LOG_LEVEL=debug

# 禁用性能监控
VITE_ENABLE_PERFORMANCE_MONITOR=false

# 禁用错误上报
VITE_ENABLE_ERROR_REPORTING=false

# 禁用用户行为分析
VITE_ENABLE_ANALYTICS=false

# ===========================================
# 图像生成配置
# ===========================================

