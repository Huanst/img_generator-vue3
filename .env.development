# 图像生成超时时间（毫秒）
VITE_IMAGE_GENERATION_TIMEOUT=120000

# ===========================================
# 用户体验配置
# ===========================================

# 启用图像预览
VITE_ENABLE_IMAGE_PREVIEW=true

# 启用历史记录
VITE_ENABLE_HISTORY=true

# 历史记录最大数量
VITE_MAX_HISTORY_COUNT=50

# 启用自动保存
VITE_ENABLE_AUTO_SAVE=true

# ===========================================
# 安全配置
# ===========================================

# 开发环境不强制HTTPS
VITE_FORCE_HTTPS=false

# 开发环境禁用内容安全策略
VITE_ENABLE_CSP=false

# 启用XSS保护
VITE_ENABLE_XSS_PROTECTION=true

